# xrandr
xrandr --output eDP --off # disable main monitor
xrandr --output eDP --auto --left-of HDMI-A-0 # enable main monitor, extend to left-of second monitor
xrandr --output HDMI-A-0 -auto --left-of eDP # enable second monitor, extend to right-of main monitor
